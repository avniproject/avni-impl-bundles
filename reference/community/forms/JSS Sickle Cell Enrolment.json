  "decisionRule" : "",
  "visitScheduleRule" : "//SAMPLE RULE EXAMPLE\n\"use strict\";\n({ params, imports }) => {\n  const programEnrolment = params.entity;\n  const scheduleBuilder = new imports.rulesConfig.VisitScheduleBuilder({\n    programEnrolment\n  });\n    const labEncounters = _.chain(programEnrolment.getEncounters(true))\n        .filter((enc) => enc.name === 'Base screening')\n        .filter((enc) => imports.moment(enc.earliestVisitDateTime).isSame(programEnrolment.enrolmentDateTime) === true)\n        .value();\n  if(labEncounters.length == 0){\n    scheduleBuilder\n        .add({\n            name: \"Base screening\",\n            encounterType: \"Base screening\",\n            earliestDate: imports.moment(programEnrolment.enrolmentDateTime).toDate(),\n            maxDate: imports.moment(programEnrolment.enrolmentDateTime).add(1, 'days').toDate()\n        });\n    }\n   \n  return scheduleBuilder.getAllUnique(\"encounterType\");\n\n};",
