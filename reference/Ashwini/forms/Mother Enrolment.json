  "decisionRule" : "//SAMPLE RULE EXAMPLE\n  \"use strict\";\n({params, imports}) => {\n    const programEnrolment = params.entity;\n    const decisions = params.decisions;\n       \n    const medicalHistory = [\n    \"Hypertension\", \"Heart-related Diseases\", \"Diabetes\", \"Sickle Cell\", \"Epilepsy\", \"Renal Disease\",\n    \"HIV/AIDS\", \"Hepatitis B Positive\"\n];\n\nconst pastComplications = [\n    \"Ante Partum Haemorrhage\", \"Intrauterine Growth Retardation\", \"Pre-term labour\", \"Prolonged labour\",\n    \"Instrumental Delivery\", \"LSCS/C-section\", \"Intrauterine death\", \"Threatened abortion\",\n    \"3 or more than 3 spontaneous abortions\", \"Still Birth\", \"Multiple Births\", \"Retained Placenta\",\n    \"Post Partum Haemorrhage (Excessive bleeding after delivery)\", \"Intrapartum Death\", \"Neonatal death within first 28 days\",\"Congenital anomaly\", \"Rh negative in the previous pregnancy\"];\n       \n    const complicationsBuilder = new imports.rulesConfig.complicationsBuilder({\n        programEnrolment: programEnrolment,\n        complicationsConcept: 'High Risk Conditions'\n    });\n\n    complicationsBuilder.addComplication(\"Puerperal sepsis\")\n        .when.valueInEnrolment(\"Obstetrics history\").containsAnswerConceptName(\"Puerperal sepsis\");\n\n    complicationsBuilder.addComplication(\"Post abortion complications\")\n        .when.valueInEnrolment(\"Obstetrics history\").containsAnswerConceptName(\"Post abortion complications\");\n\n    complicationsBuilder.addComplication(\"Under age pregnancy\").when.age.is.lessThan(18);\n\n    complicationsBuilder.addComplication(\"Old age pregnancy\").when.age.is.greaterThan(30);\n\n    complicationsBuilder.addComplication(\"Rh Negative Blood Group\")\n        .when.valueInRegistration(\"Blood group\").containsAnyAnswerConceptName(\"AB-\", \"O-\", \"A-\", \"B-\");\n        \n    complicationsBuilder.addComplication(\"Puerperal sepsis\")\n        .when.valueInEnrolment(\"Obstetrics history\").containsAnswerConceptName(\"Puerperal sepsis\");\n\n    complicationsBuilder.addComplication(\"Short Stature\")\n        .when.valueInEnrolment(\"Height\").is.lessThanOrEqualTo(145);\n  \n    complicationsBuilder.addComplication(\"Previous Abortion(s)\")\n        .when.valueInEnrolment(\"Number of abortions\").is.greaterThan(0);\n\n   complicationsBuilder.addComplication(\"Grand Multipara\")\n        .when.valueInEnrolment(\"Gravida\").is.greaterThanOrEqualTo(5);\n     \n   complicationsBuilder.addComplication(\"Primigravida\")\n        .when.valueInEnrolment(\"Gravida\").equals(1);\n    \n    decisions.enrolmentDecisions.push(complicationsBuilder.getComplications());  \n\n    const institutionalDelivery = new imports.rulesConfig.complicationsBuilder({\n        programEnrolment: programEnrolment,\n        complicationsConcept: 'Recommendations'\n    });\n\n    institutionalDelivery.addComplication(\"Institutional Delivery\").when.age.is.lessThan(18)\n        .or.when.age.is.greaterThan(30)\n        .or.when.valueInRegistration(\"Blood group\").containsAnyAnswerConceptName(\"AB-\", \"O-\", \"A-\", \"B-\")\n        .or.when.valueInRegistration(\"Medical history\").containsAnyAnswerConceptName(...medicalHistory)\n        .or.when.valueInEnrolment(\"Height\").is.lessThan(145)\n        .or.when.valueInEnrolment(\"Gravida\").is.equals(1)\n        .or.when.valueInEnrolment(\"Gravida\").is.greaterThanOrEqualTo(5)\n        .or.when.valueInEnrolment(\"Age of youngest child\").asAge.is.lessThan(1)\n        .or.when.valueInEnrolment(\"Obstetrics history\").containsAnyAnswerConceptName(...pastComplications)\n        .or.when.valueInEnrolment(\"Infertility treatment\").is.yes;\n    \n    decisions.enrolmentDecisions.push(institutionalDelivery.getComplications());\n\n\n const institutionalDeliveryReasons = new imports.rulesConfig.complicationsBuilder({\n        programEnrolment: programEnrolment,\n        complicationsConcept: 'Reason(s) for Institutional Delivery'\n    });\n\n    institutionalDeliveryReasons.addComplication(\"Under age pregnancy\").when.age.is.lessThan(18);\n    institutionalDeliveryReasons.addComplication(\"Old age pregnancy\").when.age.is.greaterThan(30);\n    institutionalDeliveryReasons.addComplication(\"Rh Negative Blood Group\").when.valueInRegistration(\"Blood group\").containsAnyAnswerConceptName(\"AB-\", \"O-\", \"A-\", \"B-\");\n    institutionalDeliveryReasons.addComplication(\"Medical history\").when.valueInRegistration(\"Medical history\").containsAnyAnswerConceptName(...medicalHistory);\n    institutionalDeliveryReasons.addComplication(\"Short Stature\").when.valueInEnrolment(\"Height\").is.lessThan(145);\n    institutionalDeliveryReasons.addComplication(\"Primigravida\").when.valueInEnrolment(\"Gravida\").is.equals(1);\n    institutionalDeliveryReasons.addComplication(\"Grand Multipara\").when.valueInEnrolment(\"Gravida\").is.greaterThanOrEqualTo(5);\n    institutionalDeliveryReasons.addComplication(\"Youngest child Less than 1 Year Old\").when.valueInEnrolment(\"Age of youngest child\").asAge.is.lessThan(1);\n    institutionalDeliveryReasons.addComplication(\"Obstetrics history\").when.valueInEnrolment(\"Obstetrics history\").containsAnyAnswerConceptName(...pastComplications);\n    institutionalDeliveryReasons.addComplication(\"Infertility treatment\").when.valueInEnrolment(\"Infertility treatment\").is.yes;\n    \n    decisions.enrolmentDecisions.push(institutionalDeliveryReasons.getComplications());\n\n\nconst reasonsBuilder = new imports.rulesConfig.complicationsBuilder({\n        programEnrolment: programEnrolment,\n        complicationsConcept: 'Refer to the hospital immediately for'\n    });\n\n    reasonsBuilder.addComplication(\"TB\")\n        .when.valueInEnrolment(\"Did she complete her TB treatment?\").containsAnswerConceptName(\"No\")\n        .or.when.valueInEnrolment(\"Has she been taking her TB medication regularly?\").containsAnswerConceptName(\"No\");\n    \n    decisions.enrolmentDecisions.push(reasonsBuilder.getComplications());\n\n    return decisions;\n};",
