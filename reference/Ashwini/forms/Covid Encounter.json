      "displayOrder" : 12.0,
      "type" : "SingleSelect",
      "rule" : "  ({params, imports}) => {\n    const programEncounter = params.entity;\n    const formElement = params.formElement;\n    \n\n    const colors = ['Green', 'Orange', 'Red'];\n    let finalColor = 'Green';\n    \n    let changeColor = (color) => {\n        if (colors.indexOf(color) > colors.indexOf(finalColor)) {\n            finalColor = color;\n        }\n    }\n    let temperature = programEncounter.getObservationReadableValue('Fever?');\n    const temperatures = programEncounter.programEnrolment.getObservationsForConceptName('Fever?');\n    let feverDays = temperatures.filter(temperature => temperature.obs === '04bb1773-c353-44a1-a68c-9b448e07ff70')\n    if (temperature === 'Yes' && feverDays.length > 3) {\n        changeColor('Red');\n    }\n    let cough = programEncounter.getObservationReadableValue('Cough?');\n    const coughh = programEncounter.programEnrolment.getObservationsForConceptName('Cough?');\n    let coughDays = coughh.filter(cough => cough.obs === '04bb1773-c353-44a1-a68c-9b448e07ff70')\n    if (cough === 'Yes' && coughDays.length > 3) {\n        changeColor('Red');\n    }\n    let saturation = programEncounter.getObservationReadableValue('Saturation value');\n       if  (saturation < 94) { \n            changeColor('Red');\n            }\n       if  (saturation == 94) { \n            changeColor('Orange');\n            }\n    let Respiratory = programEncounter.getObservationReadableValue('Respiratory rate value');\n       if  (Respiratory > 26) { \n            changeColor('Red');\n            }\n       if  (Respiratory >= 24 && Respiratory <= 26) { \n            changeColor('Orange');\n            }\n    return new imports.rulesConfig.FormElementStatus(formElement.uuid, true, finalColor);\n}",
      "mandatory" : false
    } ],
