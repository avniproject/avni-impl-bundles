  "visitScheduleRule" : "//SAMPLE RULE EXAMPLE\n\"use strict\";\n({ params, imports }) => {\n  const programEnrolment = params.entity;\n  const scheduleBuilder = new imports.rulesConfig.VisitScheduleBuilder({\n    programEnrolment\n  });\n  //add new visit schedule object to scheduleBuilder\n  scheduleBuilder\n    .add({\n      name: \"Clinical Assessment Form\",\n      encounterType: \"Clinical Assessment Form\",\n      earliestDate: programEnrolment.enrolmentDateTime,\n      maxDate: imports.moment(programEnrolment.enrolmentDateTime).add(2, 'days').toDate()\n    });\n  \n  return scheduleBuilder.getAll();\n};",
