      "rule" : "'use strict';\n({params, imports}) => {\n    const programEnrolment = params.entity;\n    const moment = imports.moment;\n    const formElement = params.formElement;\n    const _ = imports.lodash;\n    let visibility = true;\n    let value = null;\n    let answersToSkip = [];\n    let answersToShow = [];\n    let validationErrors = [];\n    const kharifUUID = '58583b46-f8a3-486a-9903-9908899d44ef';\n    const rabiUUID = 'b167e259-fc52-4f34-8043-3416692f707b';\n    const zaidUUID = 'c0f902da-88e1-4206-84ea-29a6e20ba51b';\n    let allAnswerConcepts = [kharifUUID, rabiUUID, zaidUUID];\n    let toBeShownList = [];\n\n    const enrolmentDateTime = moment(programEnrolment.enrolmentDateTime).startOf('day');\n\n    const currentMonth = enrolmentDateTime.month(); // 0 = Jan, 11 = Dec\n\n    if (currentMonth >= 6 && currentMonth <= 11) toBeShownList.push(kharifUUID);\n    if (currentMonth >= 9 || currentMonth <= 4) toBeShownList.push(rabiUUID);\n    if (currentMonth >= 3 && currentMonth <= 5) toBeShownList.push(zaidUUID);\n\n    toBeShownList = _.uniq(toBeShownList);\n\n    let toBeHidden = _.difference(allAnswerConcepts, toBeShownList);\n    \n\n    if(toBeHidden && toBeHidden.length > 0){\n        _.forEach(toBeHidden, (answer) => {\n            const answerToSkip = formElement.getAnswerWithConceptUuid(answer);\n            if (answerToSkip) answersToSkip.push(answerToSkip);\n        });\n    }\n\n    return new imports.rulesConfig.FormElementStatus(formElement.uuid, visibility, value, answersToSkip, validationErrors, answersToShow);\n};",
