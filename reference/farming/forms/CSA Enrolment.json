  "visitScheduleRule" : "\"use strict\";\n({ params, imports }) => {\n    const programEnrolment = params.entity;\n    const moment = imports.moment;\n    const scheduleBuilder = new imports.rulesConfig.VisitScheduleBuilder({programEnrolment});\n    const cropTypeUUID = '5f5f172c-3f12-4c95-8825-7b94d682ce77';\n    const kharifUUID = '58583b46-f8a3-486a-9903-9908899d44ef';\n    const rabiUUID = 'b167e259-fc52-4f34-8043-3416692f707b';\n    const zaidUUID = 'c0f902da-88e1-4206-84ea-29a6e20ba51b';\n\n    const cropType = programEnrolment.getObservationValue(cropTypeUUID);\n    \n    \n    const isAlreadyScheduled = programEnrolment.getEncountersOfType(\"Lead Farmer Monitoring\").length > 0;\n\n    if(!isAlreadyScheduled){\n\n        const earliestDate = moment(programEnrolment.enrolmentDateTime).add(30, 'days').startOf('day').toDate();\n        const maxDate = moment(earliestDate).add(7, 'days').endOf('day').toDate();\n        scheduleBuilder.add({name: \"Lead Farmer Monitoring Follow-up 1\", encounterType: \"Lead Farmer Monitoring\", earliestDate, maxDate});  \n    }\n    \n    return scheduleBuilder.getAll();\n};",
