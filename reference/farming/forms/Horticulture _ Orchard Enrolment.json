  "visitScheduleRule" : "\"use strict\";\n({ params, imports }) => {\n    const programEnrolment = params.entity;\n    const moment = imports.moment;\n    const scheduleBuilder = new imports.rulesConfig.VisitScheduleBuilder({programEnrolment});\n\n    const isAlreadyScheduled = programEnrolment.getEncountersOfType(\"Orchard Development\").length > 0;\n\n    const baseDate = programEnrolment.enrolmentDateTime;\n\n    if(!isAlreadyScheduled){\n        const enrolmentDate = moment(baseDate).startOf('day');\n        let earliestDate = null;\n        const enrolmentYear = enrolmentDate.year();\n        const enrolmentMonth = enrolmentDate.month() + 1;\n        const isBeforeApril = enrolmentMonth < 4;\n        const isAfterJune = enrolmentMonth > 6;\n        const isInBetweenAprilAndJune = enrolmentMonth >= 4 && enrolmentMonth <= 6;\n\n        if(isBeforeApril){\n            earliestDate = moment(baseDate).startOf('year').add(3, 'months').startOf('month').toDate();\n        }\n        else if(isAfterJune){\n            earliestDate = moment(baseDate).startOf('year').add(1, 'year').add(3, 'months').startOf('month').toDate();\n        }\n        else if(isInBetweenAprilAndJune){\n            earliestDate = moment(programEnrolment.enrolmentDateTime).startOf('day').toDate();\n        }\n\n        if(earliestDate) {\n            let maxDate = moment(earliestDate).startOf('year').add(5, 'months').endOf('month').toDate();\n            scheduleBuilder.add({name: \"Orchard Development Follow-up 1\", encounterType: \"Orchard Development\", earliestDate, maxDate});  \n        }\n    }\n    \n    return scheduleBuilder.getAll();\n};",
